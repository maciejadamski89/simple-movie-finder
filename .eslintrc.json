{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["next/core-web-vitals", "prettier"],
	"overrides": [],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react", "@typescript-eslint"],
	"rules": {
		"camelcase": [
			"error",
			{
				"properties": "never"
			}
		],
		"prefer-const": "error",
		"no-console": ["error", {"allow": ["warn", "error"]}]
	}
}
